{
  "name": "zip-codes-future",
  "version": "0.0.1",
  "description": "Zip code checker using futures",
  "main": "index.js",
  "scripts": {
    "build": "browserify main.js -t babelify --outfile build.js",
    "watch": "watchify main.js -v -t babelify --outfile build.js",
    "sync-serve": "browser-sync start --server --files='index.html, build.js'",
    "dev": "npm run watch & npm run sync-serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Simon Friis Vindum",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.21",
    "babelify": "^6.1.1",
    "browser-sync": "^2.26.3",
    "browserify": "^16.2.3",
    "treis": "^2.6.0",
    "watchify": "^3.11.1"
  },
  "dependencies": {
    "ramda": "^0.17.1",
    "ramda-fantasy": "^0.8.0",
    "snabbdom": "^0.7.3",
    "union-type": "^0.1.6"
  }
}
